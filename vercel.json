{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/:path(.*)\\.html",
            "destination": "/:path",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/css/satvplusClient.:hash.css",
            "destination": "/css/styles.css"
        },
        {
            "source": "/css/styles.css",
            "destination": "/css/styles.css"
        }
    ]
}